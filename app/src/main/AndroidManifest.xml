<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="floda.pl.floda3">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".Floda_LOGIN"
            android:screenOrientation="portrait"
            android:theme="@style/NOACTIO" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".Floda_SING_UP"
            android:screenOrientation="portrait"
            android:theme="@style/NOACTIO" />
        <activity
            android:name=".Floda_main"
            android:label="@string/title_activity_floda_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Starting"
            android:theme="@style/NOACTIO">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlantDetail"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".settings.ESPsettings"
            android:label="@string/title_activity_espsettings"
            android:theme="@style/AppThemeact" />
        <activity android:name=".add.Floda_add_plant"
            android:theme="@style/AppThemeact" ></activity>
    </application>

</manifest>